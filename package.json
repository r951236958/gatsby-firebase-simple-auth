{
  "name": "gatsby-firebase-simple-auth",
  "description": "Gatsby simple authentication flow using firebase",
  "version": "1.0.0",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:gatsby": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "build:tailwind": "tailwind build static/styles/index.css -o static/styles/tailwind.css"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@headlessui/react": "^0.2.0",
    "@popperjs/core": "^2.6.0",
    "@react-firebase/auth": "^0.2.10",
    "@xstate/react": "^1.2.2",
    "autoprefixer": "^10.1.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.8.0",
    "firebase": "^7.24.0",
    "gatsby": "^2.29.2",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-create-client-paths": "^2.7.0",
    "gatsby-plugin-firebase": "^0.2.0-beta.4",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-postcss": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-sharp": "^2.9.0",
    "postcss": "^8.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-firebase-hooks": "^2.2.0",
    "react-firebaseui": "^4.1.0",
    "react-helmet": "^5.2.1",
    "react-lazy-load-image-component": "^1.5.1",
    "tailwindcss": "^2.0.2",
    "xstate": "^4.15.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@iconify-icons/ic": "^1.1.1",
    "@iconify-icons/mdi": "^1.1.2",
    "@iconify/react": "^1.1.3",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby-plugin-prettier-eslint": "^1.0.4",
    "prettier": "^2.2.1"
  }
